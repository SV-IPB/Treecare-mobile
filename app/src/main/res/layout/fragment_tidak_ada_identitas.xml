<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".user.pengamatan_visual.TidakAdaIdentitasFragment">

    <TextView
        android:id="@+id/tvKet"
        android:layout_width="222dp"
        android:layout_height="wrap_content"
        android:text="Pohon belum memiliki identitas"
        android:fontFamily="@font/poppins_regular"
        android:textSize="20sp"
        android:textColor="#9f9f9f"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="100dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnTambah"
        android:layout_width="254dp"
        android:layout_height="42dp"
        android:text="+ Tambah Identitas Pohon"
        android:fontFamily="@font/poppins_medium"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvKet"
        android:layout_marginTop="31dp"
        android:background="@drawable/btn_bg_peta"/>


</androidx.constraintlayout.widget.ConstraintLayout>